<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
    <bean id="userDao" class="cn.chenwj.bean.config.javaconfig.UserDao"/>
    <bean id="logDao" class="cn.chenwj.bean.config.javaconfig.LogDao"/>
    <bean id="logonService" class="cn.chenwj.bean.config.javaconfig.LogonService"
          p:logDao-ref="userDao" p:userDao-ref="logDao"/>
</beans>
